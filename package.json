{
  "name": "pay-user-support",
  "description": "A Node.js app for sending users messages to zendesk",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^10.15"
  },
  "scripts": {
    "compile": "grunt generate-assets",
    "clean": "grunt clean",
    "server": "node ./start.js",
    "start": "npm run server",
    "dev": "nodemon start",
    "watch": "./node_modules/.bin/grunt watch",
    "test": "npm run start & cypress run",
    "test:headed": "cypress open",
    "snyk-protect": "./node_modules/.bin/snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "assets/*",
      "Gruntfile.js",
      "public/js/*"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "cy",
      "Cypress"
    ]
  },
  "dependencies": {
    "body-parser": "1.19.x",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.3.3",
    "csrf": "^3.1.0",
    "express": "4.17.x",
    "govuk-frontend": "^2.13.0",
    "helmet": "^3.21.0",
    "lodash": "^4.17.15",
    "minimist": "1.2.x",
    "morgan": "1.9.x",
    "node-zendesk": "^1.4.0",
    "nunjucks": "^3.2.0",
    "rfc822-validate": "^1.0.0",
    "serve-favicon": "^2.5.0",
    "staticify": "^3.3.4",
    "throng": "4.0.x",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cypress": "^3.4.1",
    "eslint": "^6.3.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.5.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-rewrite": "1.0.x",
    "grunt-sass": "^3.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "snyk": "^1.339.1",
    "standard": "^14.1.0"
  },
  "snyk": true
}
